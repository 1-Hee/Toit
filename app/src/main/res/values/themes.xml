<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ToIt" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2f4f4f</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name = "android:windowActionBar">false</item>
        <item name = "android:windowNoTitle">true</item>

    </style>
    <style name="starRatingBar1" parent="@android:style/Widget.RatingBar">
            <item name="android:progressDrawable">@drawable/custom_rating_body1</item>
    </style>
    <style name="starRatingBar2" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_rating_body2</item>
    </style>
</resources>