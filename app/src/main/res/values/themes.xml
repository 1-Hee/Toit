<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ToIt" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#a18cd1</item>
        <item name="colorPrimaryVariant">#a18cd1</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#a18cd1</item>
        <item name="colorSecondaryVariant">#a18cd1</item>
        <item name="colorOnSecondary">#F8EFDD</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name = "android:windowActionBar">false</item>
        <item name = "android:windowNoTitle">true</item>

    </style>
    <style name="starRatingBar1" parent="@android:style/Widget.RatingBar">
            <item name="android:progressDrawable">@drawable/custom_rating_body1</item>
    </style>

    <style name="mainTimer1" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/timer_body1</item>
    </style>

    <style name="mainTimer2" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/timer_body2</item>
    </style>

    <style name="DateDialog" parent="ThemeOverlay.MaterialComponents.MaterialCalendar"></style>

</resources>